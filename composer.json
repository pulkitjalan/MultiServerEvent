{
    "name": "jdavidbakr/multi-server-event",
    "type": "library",
    "description": "This package extends Laravel's native Command Event class to allow for managing events fired on the same system with multiple servers, preventing an event from firing more than once",
    "keywords": [
        "Scheduler",
        "Laravel",
        "Multi-Server",
        "Load Balancing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Baker",
            "email": "me@jdavidbaker.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "5.1.*|5.2.*",
        "illuminate/contracts": "5.1.*|5.2.*",
        "illuminate/database": "5.1.*|5.2.*",
        "illuminate/filesystem": "5.1.*|5.2.*",
        "illuminate/support": "5.1.*|5.2.*"
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\MultiServerEvent\\": "src/"
        }
    }
}
